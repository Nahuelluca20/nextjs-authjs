generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model comment {
  id       String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  timeAgo  DateTime @default(now())
  username String
  comment  String
}
